<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- 
  - This file is included by the struts.xml file as an example
  - of how to break up the configuration file into multiple files.
-->
<struts>
  <package name="example" namespace="/example" extends="struts-default">
  <result-types>
            <result-type name="tiles"
                class="org.apache.struts2.views.tiles.TilesResult" />
  </result-types>
    <action name="HelloWorld" class="com.skmm.app.example.HelloWorld">
      <result>/example/HelloWorld.jsp</result>
    </action>

    <action name="Login" method="execute" class="loginClass">
      <result name="input">/example/Missing.jsp</result>
      <result type="redirectAction">/example/Menu.jsp</result>
    </action>

    <action name="*WL" method="{1}" class="com.skmm.app.example.ExampleSupport">
      <result name="welcome" type="tiles">welcome</result>
      <result name="login" type="tiles">login</result>
    </action>
    
    <!-- Add additional "example" package actions here. -->
    
  </package>
</struts>
